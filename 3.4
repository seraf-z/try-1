function sumAll2(n3, n4) {
  if ( n3 > n4 ) {
    var min = n4;
    var max = n3;
  } else {
    var min = n3;
    var max = n4;
  }
  var sum = ( min + max ) * ( max - min + 1 ) / 2;
  return sum
}
